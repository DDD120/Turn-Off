{
  "name": "turn-off",
  "version": "1.0.0",
  "main": "main.js",
  "private": true,
  "description": "turn-off",
  "author": "DDD120",
  "keywords": [
    "gatsby"
  ],
  "build": {
    "icon": "src/images/favicon.ico",
    "productName": "Turn Off",
    "files": [
      "public/**/*",
      "main.js"
    ],
    "win": {
      "icon": "src/images/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "src/images/favicon.ico",
      "language": 1042
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "electron": "wait-on http://localhost:8000 && electron .",
    "electron-dev": "concurrently \"npm run develop\" \"npm run electron\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "babel-plugin-styled-components": "^2.0.7",
    "badwords-ko": "^1.0.3",
    "electron-serve": "^1.1.0",
    "framer-motion": "^7.3.4",
    "gatsby": "^5.10.0",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-layout": "^4.10.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-styled-components": "^5.23.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-full-screen": "^1.1.1",
    "react-icons": "^4.4.0",
    "react-toastify": "^9.0.8",
    "styled-components": "^5.3.5"
  },
  "engines": {
    "node": "18.0.0"
  }
